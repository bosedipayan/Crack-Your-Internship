class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n = prices.size();
        int val = INT_MAX;
        int res = 0;
        
        for(int i=0;i<n;i++)
        {
            val = min(val,prices[i]);
            res = max(res,prices[i]-val);
        }
        return res;
    }
};
